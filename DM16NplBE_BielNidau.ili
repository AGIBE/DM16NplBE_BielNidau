        SymPos: LKoord; !! in der Regel innerhalb der Flaeche positionieren
        SymOri: OPTIONAL Rotation;
        SymSize: OPTIONAL Schriftgroesse;
    NO IDENT    
    END BN_VGSym;     

    !! BN Baumstandorte (Biel: Baulinienplan)
    !! *********************************************************************

    TABLE BN_Baum =
        Verbindlichkeit: Verbindlichkeiten;
        Bezeichnung: TEXT*300;
        Abkuerzung: TEXT*24;
        KantArt: UeberlagerungArtKanton;
        DarstellungCode: TEXT*5;  !! Code 5-Stellen gefuehrt mit F = Flaeche; L = Linie
        Beschrieb: OPTIONAL TEXT*160;
        Planbeschriftung: OPTIONAL TEXT*300; !! fuer Beschriftungen auf dem Plan
        Datenverwaltungsstelle: -> Amt;
        ZustaendigeStelle: -> Amt; !!//TS:Eigentumsbeschraenkung.ZustaendigeStelle//
        Bemerkung: OPTIONAL TEXT*160;
    NO IDENT
    END BN_Baum;
    
    TABLE BN_BaumRV =
        Typ: -> BN_Baum; !!//TS:HinweisVorschrift.Eigentumsbeschraenkung//      
        Vorschrift: -> Dokument; !!//TS:HinweisVorschrift.Vorschrift//
        ArtikelNr: OPTIONAL ArtikelNummern; !!//TS:HinweisVorschrift.ArtikelNr// !! Hinweis auf spezifische Artikel. Durch Komma getrennte Liste z.B. "1, 2 a, 4, 20"
    NO IDENT
    END BN_BaumRV;
    
    TABLE BN_PktEleBaum =
        Objekt: -> BN_Baum; !!//TS:Geometrie.Eigentumsbeschraenkung//  !! Beziehung 1-mc
        Beschrieb: OPTIONAL TEXT*160;
        Genehmigung: OPTIONAL DATE;
        Inkraftsetzung: OPTIONAL DATE;
        Qualitaet: Qualitaetsstandard;
        Geometrie: LKoord; !!//TS:Geometrie.Punkt//
        Ori: Rotation;
    NO IDENT
    END BN_PktEleBaum;

    !! *********************************************************************
    !! TOPIC BN_Div Diverses Biel/Nidau
    !! *********************************************************************
    !! Ganzes Topic dient als Sammeltopf nur zur Speicherung diverser Spezialfälle, zu Konstruktions- oder zu Test-Zwecken. Enthält keine ÖREB-relevanten Festlegungen.

    TABLE BN_DivObj =
        Objektart: (Strassenlinie, Bemassungshilfslinie, Leitungstrasse);
        !! Verbindlichkeit: Verbindlichkeiten;  !! Explizit auskommentiert, damit keine Verwechslung mit ÖREB-relevanten Festlegungen.
        Bezeichnung: TEXT*300;
        Abkuerzung: TEXT*24;
        Beschrieb: OPTIONAL TEXT*160;
        Planbeschriftung: OPTIONAL TEXT*300; !! fuer Beschriftungen auf dem Plan
        !! Datenverwaltungsstelle: -> Amt;  !! Explizit auskommentiert, damit keine Verwechslung mit ÖREB-relevanten Festlegungen.
        !! ZustaendigeStelle: -> Amt;  !! Explizit auskommentiert, damit keine Verwechslung mit ÖREB-relevanten Festlegungen.
        Bemerkung: OPTIONAL TEXT*160;
    NO IDENT
    END BN_DivObj;

    TABLE BN_FlEleDivObj =
        Objekt: -> BN_DivObj;  !! Beziehung 1-mc; Objekt
        Geometrie: SURFACE WITH (STRAIGHTS, ARCS) VERTEX LKoord
            WITHOUT OVERLAPS > 0.050;
    NO IDENT
    END BN_FlEleDivObj;

    TABLE BN_LinEleDivObj =
        Objekt: -> BN_DivObj;  !! Beziehung 1-mc; Objekt
        Geometrie: POLYLINE WITH (STRAIGHTS, ARCS) VERTEX LKoord;
    NO IDENT
    END BN_LinEleDivObj;

    TABLE BN_PktEleDivObj =
        Objekt: -> BN_DivObj;  !! Beziehung 1-mc; Objekt
        Geometrie: LKoord;
        Ori: Rotation;
    NO IDENT
    END BN_PktEleDivObj;

    TABLE BN_DivObjPos = !! beschriftet Attribut Planbeschriftung
        Objekt: -> BN_DivObj; !! Beziehung 1-mc; Objekt
        TextPos: LKoord; !! in der Regel innerhalb der Flaeche positionieren
        TextOri: OPTIONAL Rotation;
        TextHAli: OPTIONAL HALIGNMENT;
        TextVAli: OPTIONAL VALIGNMENT;
        TextSize: OPTIONAL Schriftgroesse;
    NO IDENT
    END BN_DivObjPos;

    TABLE BN_DivObjSym = !! Symbol und Text mit Zonenkuerzel
        Objekt: -> BN_DivObj; !! Beziehung 1-mc; Objekt
        SymPos: LKoord; !! in der Regel innerhalb der Flaeche positionieren
        SymOri: OPTIONAL Rotation;
        SymSize: OPTIONAL Schriftgroesse;
    NO IDENT
    END BN_DivObjSym;
    
  END Nutzungsplanung.

END DM16NplBE_BielNidau.

FORMAT FREE;

CODE
  BLANK = 0X7E , UNDEFINED = DEFAULT, CONTINUE = DEFAULT;
  TID = ANY;
END.